{
  "name": "<%- moduleName %>",
  "version": "0.0.0",
  "description": "<%- dequote(moduleDesc) %>",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "scripts": {
    "test": "mocha test/{**/,/}*.js",
    "coverage": "istanbul cover _mocha test/{**/,/}*.js -- -R spec",
    "precoveralls": "istanbul cover _mocha test/{**/,/}*.js --report lcovonly -- -R spec && npm i coveralls@2",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "postcoveralls": "rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/<%- githubName %>/<%- moduleName %>"
  },
  "keywords": <%- JSON.stringify(keywords) %>,
  "author": {
    "name": "Zoltan Kochan",
    "email": "zoltan.kochan@gmail.com",
    "url": "http://kochan.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/<%- githubName %>/<%- moduleName %>/issues"
  },
  "homepage": "https://github.com/<%- githubName %>/<%- moduleName %>#readme",
  "dependencies": {},
  "devDependencies": {
    "chai": "^3.4.1",
    "istanbul": "^0.4.2",
    "mocha": "^2.3.4"
  }
}
